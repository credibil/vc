<!-- 
	Register app custom URI scheme(s).
	Needs to sit in same directory as `tauri.conf.json` to be picked up by build process
-->
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleURLName</key>
				<!-- <string>OpenID for Verifiable Credentials</string> -->
				<string>io.credibil.wallet</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<!-- register the openid-vc:// scheme -->
					<string>openid-vc</string>
				</array>
			</dict>
		</array>
	</dict>
</plist>